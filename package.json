{
  "name": "reelapps-workspace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm validate-env && pnpm -r build",
    "build:netlify": "pnpm validate-env && pnpm -r build",
    "lint": "eslint .",
    "test": "pnpm -r test",
    "ci:test": "pnpm lint && pnpm test",
    "clean": "pnpm --recursive clean",
    "validate-env": "node scripts/validate-env.js",
    "setup:dev": "node scripts/setup-env.js",
    "setup:prod": "node scripts/setup-env.js --prod"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "pnpm": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.2",
    "dotenv": "^16.6.0"
  }
}
